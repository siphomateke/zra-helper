  "version": "2.0.0-beta.2",
  "description": "A browser extension that automates various aspects of the ZRA website.",
  "scripts": {
    "dev": "vite",
    "serve:firefox": "cross-env VITE_BROWSER=firefox yarn run dev",
    "serve:chrome": "cross-env VITE_BROWSER=chrome yarn run dev",
    "build": "vue-tsc && vite build",
    "build:firefox": "cross-env VITE_BROWSER=firefox yarn run build",
    "build:chrome": "cross-env VITE_BROWSER=chrome yarn run build",
    "watch": "vite build --watch --mode development",
    "lint": "eslint lint --format stylish --mode production",
    "devtools": "vue-devtools",
    "postversion": "git push --tags",
    "test:unit": "echo \"Not implemented\""
  },
